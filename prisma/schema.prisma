datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets   = ["native", "debian-openssl-3.0.x"]
}

model work {
  id         Int     @id @default(autoincrement())
  name       String
  startDate  DateTime
  endDate    DateTime
  createdAt  DateTime   @default(now())
  updatedAt  DateTime   @updatedAt
}
